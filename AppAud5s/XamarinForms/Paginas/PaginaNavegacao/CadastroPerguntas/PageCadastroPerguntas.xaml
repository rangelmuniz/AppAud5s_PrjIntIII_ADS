<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:ext="clr-namespace:AppAud5s.XamarinForms.Paginas.PaginaNavegacao.CadastroPerguntas"
             Title="Cadastro de Perguntas"
             x:Class="AppAud5s.XamarinForms.Paginas.PaginaNavegacao.PageCadastroPerguntas">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Cadastro Perguntas"
                VerticalOptions="Start"
                HorizontalOptions="Start"/>

            <Image x:Name="Imagem01" Source="{ext:ImageResource AppAud5s.XamarinForms.Paginas.PaginaNavegacao.CadastroPerguntas.metodologiaCincoS.png}" 
                   VerticalOptions="CenterAndExpand"
                   HorizontalOptions="CenterAndExpand"/>

            <Button Text="Cadastro" TextColor="Crimson" Clicked="BtnGetCadastroModeloPergunta"/>
            <Button Text="Listar Perguntas" Clicked="GetTelaListarPerguntas" />
            <Button Text="Voltar" Clicked="VoltarPaginaAnterior"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>