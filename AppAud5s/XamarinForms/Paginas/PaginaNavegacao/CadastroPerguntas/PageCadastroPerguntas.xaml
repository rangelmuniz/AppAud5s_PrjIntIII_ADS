<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Cadastro de Perguntas"
             x:Class="AppAud5s.XamarinForms.Paginas.PaginaNavegacao.PageCadastroPerguntas">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Cadastro Perguntas"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Iniciar Cadastro" TextColor="Crimson" Clicked="ChamarSensoSeiri"/>
            <Button Text="Voltar" Clicked="VoltarPaginaAnterior"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>