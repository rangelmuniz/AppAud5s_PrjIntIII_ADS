<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Seiton(Ordenação/Arrumação)"
             NavigationPage.HasBackButton="False"
             x:Class="AppAud5s.XamarinForms.Paginas.PaginaNavegacao.CadastroPerguntas.Sensos.PageSensoSeiton">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Seiton(Ordenação/Arrumação)"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />

            <Button Text="Póximo Senso" 
                    Clicked="AvancaSenso"                    
                    FontAttributes="Bold" />
            <Button Text="Voltar" 
                    Clicked="VoltarSenso" 
                    VerticalOptions="EndAndExpand" 
                    FontAttributes="Bold" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>